<!-- thread-count = "(tests running at a time)" -->

<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd"> 
<suite name="Regression" parallel="tests" thread-count="1">
	<parameter name="website" value="https://www.facebook.com"></parameter>

	<test name="Facebook Login Test">
		<parameter name="browser" value="edge"></parameter>
		<classes>

			<class name="testng.FB_Login_DataProvider"></class>
		</classes>

	</test>
	
